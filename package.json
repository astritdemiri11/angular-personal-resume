{
  "name": "personal-resume",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "node dist/personal-resume/server/main.js",
    "build": "ng build --base-href ./ --aot --source-map true && ng run personal-resume:server",
    "compress": "gulp compress",
    "sass": "gulp sass",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "http-server": "http-server -p 8080 --gzip -c-1 dist/personal-resume/browser",
    "serveByIp": "ng serve --host 10.0.0.61 --port 4200",
    "serveByIp2": "ng serve --host 192.168.1.42 --port 4200",
    "serveAt4400": "ng serve --port 4400",
    "build:arrangement": "ng build ngx-arrangement --watch",
    "build:material-translate": "ng build ngx-material-translate --watch",
    "build:scroll-carousel": "ng build ngx-scroll-carousel --watch",
    "build:text-animation": "ng build ngx-text-animation --watch",
    "build:all": "ng build ngx-arrangement && ng build ngx-material-translate && ng build ngx-scroll-carousel && ng build ngx-text-animation",
    "dev:ssr": "env-cmd --file=\"./config/dev.env\" ng run personal-resume:serve-ssr",
    "serve:ssr": "env-cmd --file=\"./config/dev.env\" node dist/personal-resume/server/main.js",
    "build:ssr": "npm run build && ng run personal-resume:server",
    "prerender": "ng run personal-resume:prerender",
    "e2e": "ng e2e",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.1.1",
    "@angular/cdk": "^14.1.1",
    "@angular/common": "^14.1.1",
    "@angular/compiler": "^14.1.1",
    "@angular/core": "^14.1.1",
    "@angular/flex-layout": "^14.0.0-beta.40",
    "@angular/forms": "^14.1.1",
    "@angular/material": "^14.1.1",
    "@angular/platform-browser": "^14.1.1",
    "@angular/platform-browser-dynamic": "^14.1.1",
    "@angular/platform-server": "^14.1.1",
    "@angular/router": "^14.1.1",
    "@angular/service-worker": "^14.1.1",
    "@ngrx/effects": "^14.3.0",
    "@ngrx/entity": "^14.3.0",
    "@ngrx/store": "^14.3.0",
    "@nguniversal/express-engine": "^14.0.3",
    "cors": "^2.8.5",
    "express": "^4.15.2",
    "express-useragent": "^1.0.15",
    "hammerjs": "^2.0.8",
    "ngx-arrangement": "^1.0.5",
    "ngx-material-translate": "^1.0.2",
    "ngx-scroll-carousel": "^1.6.3",
    "ngx-text-animation": "^1.0.2",
    "nodemailer": "^6.7.8",
    "rxjs": "^7.5.6",
    "tslib": "^2.4.0",
    "tsparticles-engine": "^2.2.2",
    "tsparticles-slim": "^2.2.2",
    "zone.js": "^0.11.7"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.1.1",
    "@angular/cli": "^14.2.1",
    "@angular/compiler-cli": "^14.1.1",
    "@cypress/schematic": "^2.1.1",
    "@ngrx/schematics": "^14.0.2",
    "@nguniversal/builders": "^14.0.3",
    "@types/express": "^4.17.0",
    "@types/express-useragent": "^1.0.2",
    "@types/hammerjs": "^2.0.41",
    "@types/jasmine": "^4.3.0",
    "@types/node": "^18.7.15",
    "@types/nodemailer": "^6.4.6",
    "cypress": "latest",
    "env-cmd": "^10.1.0",
    "gulp": "^4.0.2",
    "gulp-gzip": "^1.4.2",
    "jasmine-core": "^4.3.0",
    "karma": "^6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "^5.1.0",
    "karma-jasmine-html-reporter": "^2.0.0",
    "ng-packagr": "^14.1.1",
    "typescript": "^4.6.4"
  }
}
